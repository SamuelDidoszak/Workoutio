<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/mediumDark"/>
            <stroke android:width="1dp" android:color="@color/hardDark"/>
        </shape>
    </item>
    <item android:left="1dp">
        <shape android:shape="rectangle">
            <solid android:color="#00000000"/>
            <stroke android:width="1dp" android:color="@color/mediumDark"/>
        </shape>
    </item>
</layer-list>